	<queryString>
		<![CDATA[SELECT nota.MATRICULA, aluno.aluno, aluno.nivel,aluno.SERIE,nota.DISCIPLINA, nota.ACERTOS, 
nota.NOTA, nota.CONFERENCIA_DATA 
FROM aluno INNER JOIN nota on aluno.IDALUNO = nota.MATRICULA WHERE nota.SIMULADO = $P{cbmSim}]]>
